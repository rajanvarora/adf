{
	"name": "ASQL_To_OnPrem",
	"properties": {
		"activities": [
			{
				"name": "CopyData_ASQL_To_OnPrem",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "AzureSqlSource",
						"queryTimeout": "02:00:00"
					},
					"sink": {
						"type": "SqlServerSink",
						"sqlWriterStoredProcedureName": "[lego].[usp_upsert_themes]",
						"sqlWriterTableType": "lego.themes",
						"storedProcedureTableTypeParameterName": "themes"
					},
					"enableStaging": false
				},
				"inputs": [
					{
						"referenceName": "ASQLNewTable",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "OnPremSqlSeverTable",
						"type": "DatasetReference"
					}
				]
			}
		],
		"annotations": []
	},
	"type": "Microsoft.DataFactory/factories/pipelines"
}